{
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-DemospringmvcApplication<demospringmvc>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "mainClass": "com.example.demospringmvc.DemospringmvcApplication",
            "projectName": "demospringmvc",
            "args": "--spring.profiles.active=dev"
        },
        {
            "type": "java",
            "name": "Spring Boot-Aad3authApplication<aad3auth>",
